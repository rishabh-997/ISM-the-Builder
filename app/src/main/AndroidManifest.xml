<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ismthebuilder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".LogIn.LogIn.LogInActivity"
            android:screenOrientation="portrait"
            android:label="LogIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogIn.Constructor.ConstructorActivity"
            android:screenOrientation="portrait"
            android:label="Constructor"/>
        <activity android:name=".LogIn.ProgressLease.ProgressActivity"
            android:label="Progress"
            android:screenOrientation="portrait"/>
    </application>

</manifest>